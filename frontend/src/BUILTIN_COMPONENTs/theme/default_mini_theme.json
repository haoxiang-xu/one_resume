{
  "light_mode": {
    "icon": { "color": "#222222" },
    "switch": {
      "transition": "background-color 0.36s cubic-bezier(0.32, 1, 0.32, 1), border 0.36s cubic-bezier(0.32, 1, 0.32, 1)",
      "position": "relative",
      "height": 32,
      "width": 52,
      "backgroundColor": "#CCCCCC",
      "backgroundColor_on": "#65C467",
      "borderRadius": 20,
      "color": "#FFFFFF",
      "boxShadow": "inset 0 2px 3px rgba(0, 0, 0, 0.16)",
      "cursor": "pointer"
    },
    "button": {
      "position": "relative",
      "display": "inline-block",
      "alignItems": "center",
      "justifyContent": "center",
      "flexDirection": "column",
      "backgroundColor": "#1876D2",
      "color": "#FFFFFF",
      "borderRadius": 12,
      "border": "1px solid #CCCCCC",
      "boxShadow": "0 2px 3px rgba(0, 0, 0, 0.16)",
      "padding": "8px",
      "cursor": "pointer"
    },
    "font": {
      "color": "#222222"
    },
    "backgroundColor": "#FFFFFF"
  },
  "dark_mode": {
    "icon": { "color": "#CCCCCC" },
    "switch": {
      "transition": "background-color 0.36s cubic-bezier(0.32, 1, 0.32, 1), border 0.36s cubic-bezier(0.32, 1, 0.32, 1)",
      "position": "relative",
      "height": 32,
      "width": 52,
      "backgroundColor": "#494949",
      "backgroundColor_on": "#65C467",
      "borderRadius": 20,
      "color": "#222222",
      "boxShadow": "inset 0 2px 3px rgba(0, 0, 0, 0.16)",
      "cursor": "pointer"
    },
    "button": {
      "position": "relative",
      "display": "inline-block",
      "alignItems": "center",
      "justifyContent": "center",
      "flexDirection": "column",
      "backgroundColor": "rgba(255, 255, 255, 0.16)",
      "backdropFilter": "blur(8px)",
      "color": "#FFFFFF",
      "borderRadius": 10,
      "boxShadow": "0 4px 16px rgba(0, 0, 0, 0.32)",
      "padding": "8px",
      "cursor": "pointer"
    },
    "font": {
      "color": "#CCCCCC"
    },
    "backgroundColor": "#21252b"
  }
}
